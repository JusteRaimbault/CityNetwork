	Journal = {PLoS ONE},
	Month = {06},
	Number = {6},
	Pages = {1-21},
	Publisher = {Public Library of Science},
	Title = {PSOLA: A Heuristic Land-Use Allocation Model Using Patch-Level Operations and Knowledge-Informed Rules},
	Url = {http://dx.doi.org/10.1371%2Fjournal.pone.0157728},
	Volume = {11},
	Year = {2016},
