	Journal = {Urban Studies},
	Number = {13},
	Pages = {2819-2839},
	Title = {Simulating Urban Networks through Multiscalar Space-Time Dynamics: Europe and the United States, 17th-20th Centuries},
	Url = {http://dx.doi.org/10.1177/0042098010377366},
	Volume = {47},
