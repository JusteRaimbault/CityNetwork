	Month = Dec,
	Number = {2010EVRY0013},
	School = {{Universit{\'e} d'Evry-Val d'Essonne}},
	Title = {{Comparer les morphog{\'e}n{\`e}ses urbaines en Europe et aux {\'E}tats-Unis par la simulation {\`a} base d'agents -- Approches multi-niveaux et environnements de simulation spatiale}},
